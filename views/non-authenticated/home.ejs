<!DOCTYPE html>
<html lang="en">
<head>
  <meta charset="UTF-8" />
  <meta name="viewport" content="width=device-width, initial-scale=1.0" />
  <title>Cookmaster - Home</title>
</head>
<body>
  <%- include('navbar/guestnavbar') %>
  <h1>Receitas</h1>
  <a href="/recipes/search">
    <button type="button">Buscar Receitas</button>
  </a>
  <% recipes && recipes.forEach((recipe) => { %>
    <p><%= recipe.recipeName %></p>
    <p><%= recipe.userName %></p>
    <a href=<%= `/recipes/${recipe.recipeId}` %>>Ver detalhes</a>
  <% }) %>
</body>

</html>
